      ]
    },
    {
      "parameters": {
        "jsCode": "// Get aggregated Spotify data from input\nconst spotifyAggregated = $input.first().json;\n\n// Get all activities from Merge Both Sources\nconst activities = $('Merge Both Sources').all();\n\n// Create one output item per activity, each with all Spotify tracks\nreturn activities.map(activity => ({\n  json: {\n    object_type: 'activity',\n    object_data: activity.json,\n    spotify_recently_played: { items: spotifyAggregated }\n  }\n}));"
      },
